<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="car" class="ua.com.poseal.penetration.CarTesla">
        <property name="model" value="X11"/>
        <!-- <property name="age" value="2" /> -->
        <property name="device" ref="device"/>
    </bean>
    <bean id="device" class="ua.com.poseal.penetration.IPadDevice">
        <property name="model" value="apple"/>
    </bean>
</beans>